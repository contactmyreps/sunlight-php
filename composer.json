{
    "name": "contactmyreps/sunlight-php",
    "type": "library",
    "description": "A PHP wrapper for Sunlight Foundation's API",
    "keywords": [
        "contactmyreps",
        "sunlight-php"
    ],
    "homepage": "https://github.com/contactmyreps/sunlight-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Ed Fialkowski",
            "email": "edfialk@gmail.com",
            "homepage": "http://edfialk.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "edfialk\\sunlight-php\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "edfialk\\sunlight-php\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
